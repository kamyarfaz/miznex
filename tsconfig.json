{
  "compilerOptions": {
    "target": "ESNext",
    "types": [
      "@serwist/next/typings"
    ],
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "webworker"
    ],
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "ignoreDeprecations": "6.0",
    "allowJs": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "src/assets/svg/HeroImgBackSVG.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
